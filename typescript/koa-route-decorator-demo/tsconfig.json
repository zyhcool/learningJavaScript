{
    "compilerOptions": {
        "module": "commonjs",
        "esModuleInterop": true,
        "target": "es2017",
        "noImplicitAny": false,
        "moduleResolution": "node",
        "sourceMap": true,
        "outDir": "dist",
        "baseUrl": "./router-demo-copy",
        "paths": {
            "*": ["node_modules/*", "src/types/*"]
        },
        "lib": ["es2017"],
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "skipLibCheck": true
    },
    "include": ["src/**/*"],
    "experimentalDecorators": true
}
